@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@Base.Resources.Privacy.PrivacyPolicyHeader</h1>

<p>@Base.Resources.Privacy.PrivacyPolicy</p>


@DateTime.Now.ToLongDateString()


<table class="table">
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>